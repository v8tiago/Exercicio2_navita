@model GP.App.ViewModels.MarcaViewModel

@{
    ViewData["Title"] = "ListaPatrimonios";
}

<h1>Patrimônios</h1>

<table class="table table-hover">
    <thead class="thead-dark">
        <tr>
            <th>

            </th>
            <th>
                @Html.DisplayNameFor( model => model.Patrimonios )
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Patrimonios)
        {
            <tr>
                <td>
                    @Html.DisplayFor( modelItem => item.Nome )
                </td>
                <td>
                    @Html.DisplayFor( modelItem => item.Descricao )
                </td>
            </tr>
        }
    </tbody>
</table>
